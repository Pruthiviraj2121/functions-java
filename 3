package com.company;

public class p2 {
    public void display1() {
        System.out.println("the function is without parameters");
    }

    public void display2(int a, int b) {
        int c = a*b;
        System.out.println("the alue of c is" + c);
    }
    public static void main(String[] args){
        p2 j = new p2();
        j.display1();
        j.display2(9,7);
    }
}
