package com.company;

public class p1 {
    public static int escape(int x , int y) {
        int z;
        z  = x * y;
        return z;
    }


    public static void main(String[] args){
        int a = 10;
        int b = 20;
        int c;
        c = escape(a , b);
        int a1 = 2;
        int b1 = 4;
        int c1;
        c1 = escape(a1,b1);
        System.out.println("the execute value is" + c );
        System.out.println("the execute value is" + c1 );
    }
